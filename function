         //Assignment1
         function getBmiCategory(weight,height){
            var bmi = weight/(height**2);
            if (bmi < 18.5){
               return "Underweight";
            }else if(bmi < 25){
               return "Normal weight";
            }else if(bmi < 30){
               return "Overweight";
            }else{
               return "Obesity";
            }
         }
          console.log(getBmiCategory(70, 1.75));

         //Assignment2
         function getGrade(score){
            var grade;
            if(score > 70 && score <=100){
               grade = "A";
            }else if(score <= 70 && score > 60){
               grade = "B";
            }else if(score <= 60 && score > 50){
               grade = "C";
            }else if(score <= 50 && score > 40){
               grade = "D";
            }else if(score <= 40 && score >= 0){
               grade = "F";
            }
            else{
               grade = "Invalid score"
            }
            return grade;
         }
         console.log(getGrade(30));

         //Ternary operation method1
         function checkheight(height){
            var result = height > 6 ? "tall":"short";
            return result;
         }

         //Ternary operation method2
         function getGrade(score){
            return score > 70 && score <=100 ? "A":
            score <= 70 && score > 60 ? "B":
            score <= 60 && score > 50 ? "C":
            score <= 50 && score > 40 ? "D":
            score <= 40 && score >= 0 ? "F":
            "Invalid score";
         }
         console.log(getGrade(30));

