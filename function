         //Function expression
         var apple = function(e) {
            return e + "thank you";
         }

         //Arrow function
         var apple = (e) => e + "thank you";

         //Function expression
         var converttoarrowfunction = function(param1){
            return param1 * 2;
         }

         //Arrow function
         var converttoarrowfunction = (param1) => {
            param1 * 2;
         }

         //Immediately invoked function expression(IIFE)
         (function(){
            return 4*2;
         })

