         # Finding the factorial and the sum of element in an array
         var input = [5,4,8,6,3,5];
         function factorial(input){
            var result = input[0];
            for(var i = 1;i < input.length;i++){
               result = result * input[i];
            }
            return result;
         }
         function sumofarray(input){
            var sumresult = 0;
            for(element of input){
               sumresult = factorial(element);
            }
            return sumresult;
         }

