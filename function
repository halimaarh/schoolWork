         function year(start,end){
            var arr=[];
            for(var i = start; i<=end; i++)
               if(i%4==0){
                  arr.push(i)
               }
               return arr;
         }
         console.log(year(2002,2024))

         function sumArray(num){
            var sum=0;
            for(let i =0; i < num.length; i++){
            sum = sum + num[i];
            }
            console.log(sum)
         }sumArray([1,3,5])

         function sumArray(num){
            var sum=0;
            for(let i =0; i < num.length; i++){
            sum = sum + (num[i] ** 2);
            }
            console.log(sum)
         }sumArray([1,3,5])


         var arr1  = [3,'a','a','a',2,3,'a',3,'a',2,4,9,3]
         var mostFrequentItem;
         function findFrequent(arr){
            var maxCount = 0;
            for(var i = 0; i < arr.length; i++){
               var count = 0;
               for(var j = 0; j < arr.length; j++){
                  if(arr[j] === arr[i]){
                     count++;
                  }
               }
               if(count > maxCount){
                  maxCount = count;
                  mostFrequentItem = arr[i];
               }
            }
            return mostFrequentItem;
         }
         console.log(findFrequent(arr1))

